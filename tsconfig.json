{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, 
"app/login/page.tsx", 
"app/dashboard/page.jsx", 
"app/layout.js", 
"app/page.js"
, "app/dashboard/new/page.js", "app/dashboard/listings/page.js", "app/components/carCard.jsx", "app/components/card.jsx", "components/ui/card.jsx", "app/components/ads.jsx", "app/components/bentoGrid.jsx", "app/components/calltoaction.jsx", "app/components/payment-modal.jsx", "app/dashboard/components/mobile-sidebar.jsx"],
"exclude": [
  "node_modules"
]
}
